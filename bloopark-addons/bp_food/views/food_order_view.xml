<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<odoo>


    <!-- Order Page -->

    <!-- Tree view of Lunch View Extended -->
    <record id="view_food_order_tree" model="ir.ui.view">
        <field name="name">lunch_product.tree</field>
        <field name="model">lunch.order</field>
        <field name="inherit_id" ref="lunch.lunch_order_view_tree"/>
        <field name="arch" type="xml">
            <field name="date" position="after">
                <field name="table" select="0"/>
            </field>
        </field>

    </record>



</odoo>

